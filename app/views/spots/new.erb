<h1>spots#new</h1>

<%= form_with model:@spot, local:true do |f| %>

  <h4>スポット名</h4>
  <%= f.text_field :spot_name %>

  <h4>住所</h4>
  <%= f.text_field :address %>

  <h4>comment</h4>
  <%= f.text_area :comment %>
  
  <h4>画像</h4>
  <!--プレビューの追加予定-->
  <%= f.attachment_field :post_images_images, multiple: true %>
  
  <%= f.submit '投稿' %>

<% end %>